[
    {
        "command": "adb shell am broadcast -a com.oculus.vrpowermanager.prox_close",
        "displayOutputWindow": false,
        "name": "Disable Proximity Sensor"
    },
    {
        "command": "adb shellam broadcast -a com.oculus.vrpowermanager.automation_disable",
        "displayOutputWindow": false,
        "name": "Enable Proximity Sensor"
    },
    {
        "command": "adb shell setprop debug.oculus.experimentalEnabled 1",
        "displayOutputWindow": false,
        "name": "Enable Experimental Features"
    }
]
